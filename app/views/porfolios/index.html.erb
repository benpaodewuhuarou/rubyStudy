<h1>protfolio</h1>

<%= link_to "Create new Item",new_porfolio_path %>

<% @portfolio_items.each do |portfolio_item| %>
	<p><%= link_to portfolio_item.title, porfolio_show_path(portfolio_item) %></p>
	<p><%= portfolio_item.subtitle %></p>
	<%= image_tag portfolio_item.thumb_image unless portfolio_item.thumb_image.nil? %>
	<%= link_to "Edit",edit_porfolio_path(portfolio_item) %>
	<%= link_to "Delete",porfolio_path(portfolio_item.id),method: :delete,data:{confirm:'are you sure?'} %>
<% end %>